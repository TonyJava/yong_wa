<h1>Login</h1>
<%= form_for(@admin_manage_user, :url=>admin_logins_path)  do |f|%>
  <div class="field">
    <%= f.label :user_name %><br>
    <%= f.text_field :user_name %>
  </div>
  <div class="field">
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </div>
  <div class="actions">
    <%= f.submit :Login %>
  </div>
<% end %>